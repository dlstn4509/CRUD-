<%- include('../inc/head.ejs') %>
<%- include('../inc/header.ejs') %>
<div class="list-wrapper wrapper">
  <%- include('../inc/title-bar.ejs') %>
  <table class="table table-striped text-center">
    <thead>
      <tr>
        <th>번호</th>
        <th>아이디</th>
        <th>내용</th>
        <th>등록일</th>
      </tr>
    </thead>
    <tbody>
      <% for(let board of boards) { %> 
        <tr class="board-tr" data-id="<%= board.id %>">
          <td><%= board.id %></td>
          <td><%= board.userId %></td>
          <td><%= board.content %></td>
          <td><%= board.createdAt %></td>
        </tr>
        <% } %> 
    </tbody>
  </table>
</div>
<%- include('../inc/foot.ejs') %>
